<?xml version="1.0" encoding="utf-8" ?>
<ruleBase
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://github.com/EFS-Extensions/xmls/affinity.xsd"
  xmlns:g  ="http://github.com/EFS-Extensions/xmls/global.xsd"
  xsi:noNamespaceSchemaLocation="file://./Xsds/AffinitySchema.xsd">
  <metaInfo>
    <g:label>Simple Affinity Schema</g:label>
    <g:author>Marek Vajgl</g:author>
    <g:description>
      Testing configuration
    </g:description>
  </metaInfo>
  <affinity>
    <rule title="Mozilla" regex="Firefox" roll="0-4" />
    <rule title="Idle" regex="Idle" roll="" />
    <rule title="Other" regex=".+" roll="6-64" />
  </affinity>
  <priority>
    <rule title="?" regex=".+" priority="High" />
  </priority>
</ruleBase>
